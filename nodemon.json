{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".png",
    ".jpg",
    ".log",
    ".json",
    "www",
    ".git",
    "node_modules",
    "frontend",
    ".scaffold",
    "_gulp",
    "stores",
    ".tmp",
    "uploads",
    ".vue"
  ],
  "verbose": true,
  "events": {
